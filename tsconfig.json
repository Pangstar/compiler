{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "noImplicitAny": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "preserveConstEnums": true,
    "pretty": true
  }, 
  "exclude": [
    "typings/browser.d.ts",
    "typings/browser",
    "node_modules",
    "bin",
    "tests",
    "src/ast"
  ]
}
